<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/beans/p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- 1method to put values -->
	<bean name="st1" class="com.practice.primitives.Student" p:id="101" p:name="Kartik" p:address="India">
	</bean>	
	
	<!-- 2 method to put values -->
	<bean name="st2" class="com.practice.primitives.Student">
		<property name="id" value="101" />			
		<property name="name" value="Kirtiraj" />			
		<property name="address" value="India" />
				
	</bean>	
	<!-- 3 method to put values -->
	<bean name="st3" class="com.practice.primitives.Student">
		<property name="id">
			<value>103</value>	
		</property>
		<property name="name">
			<value>Aniket</value>	
		</property>
		<property name="address">
			<value>India</value>	
		</property>	
	</bean>	
</beans>