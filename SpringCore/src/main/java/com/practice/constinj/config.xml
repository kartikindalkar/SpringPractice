<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/beans/p"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean name="ce" class="com.practice.constinj.Certificate">
		<constructor-arg value="1"/>
		<constructor-arg value="Java"/>
	</bean>
	
	<bean name="st" class="com.practice.constinj.Student">
		<constructor-arg value="1" type="int"/>
		<constructor-arg value="kartik"/>		
		<constructor-arg>
			<list>
				<value>Java</value>
				<value>Python</value>
			</list>
		</constructor-arg>
		<constructor-arg ref="ce"/><!-- Object ko set kiya-->
	</bean>	
</beans>